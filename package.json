{
  "name": "react2amp",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "type/index.d.ts",
  "scripts": {
    "ci-build": "npm run build:bundle && npm run build:type",
    "ci-lint": "npm run lint && npm run prettier",
    "ci-publish": "npx semantic-release",
    "build:bundle": "rollup -c ./config/rollup.config.js",
    "build:type": "tsc --project ./config/tsconfig.type.json",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "prettier": "prettier --check ./**/*.{ts,tsx,js,json}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx,js,json}": [
      "prettier --write",
      "git add"
    ],
    "./**/*.{ts,tsx,js}": "eslint --max-warnings 0 --no-ignore"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danhuang1202/react2amp"
  },
  "keywords": [
    "react"
  ],
  "author": "danhuang",
  "license": "ISC",
  "dependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "string.prototype.matchall": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^11.13.21",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-jest": "^24.9.0",
    "codecov": "^3.6.1",
    "commitizen": "^4.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.0",
    "prettier": "^1.17.0",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "semantic-release": "^15.13.21",
    "typescript": "^3.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
